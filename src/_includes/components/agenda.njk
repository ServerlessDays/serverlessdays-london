<div class="flex-ns mv3 mv4-l">
    <div class="w-100 w-05-l dn db-l"> 
    </div>
    <div class="w-100 w-90-l ph3 ph0-ns">
        <h2 class="f2 mv3">Agenda</h2>
        <div class="pt2 pt3-ns">
            {% for session in agenda %}
                <span class="h4 tl f5 ba bw1 b--transparent dib w-100 w-auto-ns">
                    <div class="dt w-100 h-100 contrastBkg contrastColor">
                        <div class="dt-row">
                            <div class="dtc pa2 v-mid">
                                <div class="f5 b">
                                    {{ session.time }}
                                </div>
                            </div>
                            <div class="dtc pv2 pl2 pr3 v-mid">
                                <div class="f5 i">
                                    {{ session.name }}
                                </div>
                                <div class="f5 b">
                                    {% for speaker in session.speaker %}
                                        <div><a class="no-underline contrastColor" href="{{speaker.link}}">{{ speaker.name }}</a></div>
                                    {% endfor %}
                                </div>
                            </div>
                        </div>
                    </div>
                </span>
            {% endfor %}
        </div>
    </div>
    <div class="w-100 w-05-l dn db-l">
    </div> 
</div>